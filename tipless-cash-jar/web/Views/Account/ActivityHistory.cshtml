
@{
    ViewBag.Title = "ActivityHistory";
}


<section class="scrolling-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>Account Activity History</h1>
            </div>
        </div>

        <div class="row">
            <div class="col-md-offset-2 col-lg-8">
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th class="text-center">Date</th>
                            <th class="text-center">Time</th>
                            <th class="text-center">Amount</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope=row class="text-center">05/01/2016</th>
                            <td>11:35:45 AM</td>
                            <td>$ 2.00</td>
                        </tr>
                        <tr>
                            <th scope=row class="text-center">05/13/2016</th>
                            <td>04:22:02 PM</td>
                            <td>$ 2.00</td>
                        </tr>
                        <tr>
                            <th scope=row class="text-center">06/01/2016</th>
                            <td>09:05:57 AM</td>
                            <td>$ 4.00</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
